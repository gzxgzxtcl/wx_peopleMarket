/* pages/oneself/oneself.wxss */

.headInfo {
  width: 100%;
  background-color: #fff;
  padding: 44rpx 0 44rpx 60rpx;
  box-sizing: border-box;
}

.userAvatar {
  display: block;
  width: 112rpx;
  height: 112rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
  margin-right: 60rpx;
}

.userNickName {
  font-size: 40rpx;
  color: #111;
}

.iconR {
  width: 24rpx;
  height: 44rpx;
  float: right;
  margin-top: 32rpx;
  margin-right: 60rpx;
}

.userTp {
  height: 40rpx;
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
  text-align: left;
  margin-left:190rpx;
}

.controlList {
  width: 100%;
  font-size: 30rpx;
  color: #111;
}

.controlList li {
  display: block;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  border-top: 1rpx solid #e6e6e6;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.iconImg {
  width: 44rpx;
  height: 44rpx;
  margin-top: 14px;
  margin-right: 13px;
  float: left;
}
