/* pages/oneself/oneself.wxss */

.headInfo {
  width: 100%;
  background-color: #fff;
  padding: 44rpx 0 44rpx 60rpx;
}

.userAvatar {
  display: block;
  width: 112rpx;
  height: 112rpx;
  border-radius: 50%;
  overflow: hidden;
  float: left;
  margin-right: 60rpx;
}

.userNickName {
  font-size: 40rpx;
  color: #111;
}

.userTp {
  font-size: 28rpx;
  color: #333;
  margin-top: 20rpx;
}

.controlList {
  width: 100%;
  font-size: 30rpx;
  color: #111;
}

.controlList li {
  display: block;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  border-top: 1rpx solid #e6e6e6;
  padding-left:30rpx;
}
