.hours-list .bottom-info .hours-address,.rim-list .bottom-info .hours-address {
  font-family: 'PingFangTC-Light';
  font-size: 20rpx;
  color: #333;
  margin-top: 5rpx;
}

.hours-list .bottom-info .hours-address .hours-address-icon,.rim-list .bottom-info .hours-address .hours-address-icon {
  font-size: 20rpx;
}

.last-hourse-list{
  padding-bottom: 0;
}
.last-hourse-list .hours-info{
  border: none;
}
.rim-list:last-child{
  padding-bottom: 0;
}
.rim-list:last-child .hours-info{
  border: none;
}


.bgpack {
  display: flex;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.5);
  font-size: 30rpx;
  z-index: 10;
}

.bgpack .wicket {
  width: 560rpx;
  height: 334rpx;
  box-sizing: border-box;
  margin: auto;
  padding: 40rpx 0rpx 0rpx 0rpx;
  border-radius: 6rpx;
  background-color: #F8F8F8;
}

.bgpack button {
  display: inline-block;
  width: 50%;
  height: 100rpx;
  background: none;
  border-radius:0;
  font-size: 36rpx;
}

.bgpack button::after {
  border: none;
}
