  /* display: inline-block;
  padding: 0 10rpx;
  margin: 0 10rpx 0 0;
  border-radius: 4rpx;
  border: 1rpx solid #979797;
  font-family: 'PingFangTC-Light';
  font-size: 20rpx;
  color: #333;
  line-height: 28rpx;
  letter-spacing: 0.2rpx;
  text-align: center;
  margin-bottom: 5rpx; */
