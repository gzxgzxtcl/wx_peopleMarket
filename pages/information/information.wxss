/* 模态窗 */
.model-view{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: center;
}
.model-view .shadow{
 
  height: 100%;
  width: 100%; 
  position: fixed;
  top: 0;
  bottom: 0;
  margin: auto;
   background: rgba(0, 0, 0, 0.3);
}
.model-view .model-content{
  display: block;
  box-sizing: border-box;
  width: 80%;
  position: absolute;
  margin:auto;
  background: #ffffff;
  border-radius: 10rpx;
  text-align: center;
  font-family: PingFang-SC-Medium;
  font-size: 28rpx;
  color: #333333;
}
.model-view .model-content view:first-child{
  overflow-y: scroll;
  width: 93%;
  min-height: 50rpx;
  max-height: 500rpx;
  padding: 20rpx;
}
.model-view .bottom{
  width: 100%;
  height: 100rpx;
  border-top: 2rpx solid #cccccc;
  text-align: center;
  line-height: 100rpx;
  color: #1670b2;
}
