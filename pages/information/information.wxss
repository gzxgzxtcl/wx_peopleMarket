/* 详情样式 */

.info {
  margin-top: -40rpx;
  border-radius: 20rpx 20rpx 0 0;
  position: relative;
  overflow: hidden;
  padding-left: 40rpx;
  padding-right: 40rpx;
  background: #fff;
}

.info .name-money {
  /* height: 56rpx; */
  margin-top: 28rpx;
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
}

.info .name-money .name {
  width: 46%;
  display: flex;
  align-items: flex-start;
  /* padding-right: 20rpx; */
}

.info .name-money .name text {
  font-family: 'PingFang-SC-Medium';
  font-size: 40rpx;
  color: #333;
  line-height: 56rpx;
}

.info .name-money .name view {
  width: 64rpx;
  height: 26rpx;
  background: #20a0ff 60%;
  text-align: center;
  line-height: 26rpx;
  font-size: 18rpx;
  font-family: 'PingFangTC';
  border-radius: 4rpx;
  margin-left: 10rpx;
  margin-bottom: 8rpx;
  color: #fff;
  margin-top: 20rpx;
}

.info .name-money .money {
  display: flex;
  height: 100%;
  flex: 1;
  align-items: flex-end;
  justify-content: flex-end;
  padding-left: 5rpx;
}

.info .name-money .money view:first-child {
  height: 34rpx;
  line-height: 34rpx;
  font-size: 24rpx;
  font-family: 'PingFangTC-Regular';
  color: #999;
  margin-right: 14rpx;
}

.info .name-money .money .hours-money {
  font-size: 32rpx;
  font-family: 'PingFangTC-Regular';
  color: #fb1414;
  /* margin-left: 14rpx; */
}

.info .name-money .money view:nth-last-child(2) {
  font-size: 24rpx;
  font-family: 'PingFangTC-Regular';
  color: #333;
  line-height: 34rpx;
  margin-left: 4rpx;
}

.info .name-money .money view:last-child {
  width: 24.2rpx;
  height: 24.2rpx;
  border-radius: 50%;
  margin-left: 10rpx;
  margin-bottom: 6rpx;
  display: flex;
}

.info .name-money .money view:last-child image {
  width: 100%;
  height: 100%;
}

/* 标签样式 */

.information .tag {
  display: flex;
  /* height: 120rpx; */
  align-items: center;
  flex-wrap: wrap;
  margin-top: 36rpx;
}

.information .tag view {
