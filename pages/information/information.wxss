/* pages/information/information.wxss */
@import '../../utils/icon.wxss';

.information {
  width: 100%;
  overflow-x: hidden;
  background: #fff;
}

.information .num {
  background: #000;
  width: 50rpx;
  height: 24rpx;
  font-size: 18rpx;
  font-family: 'PingFangTC';
  text-align: center;
  line-height: 24rpx;
  color: #fff;
  border-radius: 12rpx;
  position: absolute;
  z-index: 30;
  right: 24rpx;
  top: 308rpx;
}

swiper {
  width: 810rpx;
  height: 403rpx;
