/* 户型图页面样式 */

page{
  height: 100%;
}
.house-img-page{
  width: 100%;
  height: 100%;
  background: #000000;
  position: relative;
}
.house-img-page .house-img{
  width: 100%;
  height: 100%;
}
swiper{
  width: 100%;
  height: calc(100% - 74rpx);
  padding-bottom: 70rpx;
  display: flex;
  align-items: center;
}
.swiper-item{
  width: 100%;
  height: 600rpx;
}
.swiper-item view{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  overflow: scroll;
}

.house-img-page .house-img image{
  width: 100%;
}
.house-img-page .tabbar{
  font-size: 28rpx;
  font-family: 'PingFang-SC-Medium';
  color: #ffffff;
  background: #000000;
  height: 74rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  padding-bottom: 32rpx;
  justify-content: space-around;
}
.house-img-page .tabbar .active{
  color: #20A0FF;
}
.house-img-page .tabbar>view{
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  margin-top: 6rpx;
}
.house-img-page .tabbar>view .icon{
  width: 19.2rpx;
  height: 12rpx;
  display: flex;
}
.house-img-page .tabbar>view image{
  width: 100%;
  height: 100%;
}
